{
    "applications": {
        "gecko": {
            "id": "two-fingers-gestures@pea3nut.org",
            "strict_min_version": "52.0"
        }
    },
    "author": "__MSG_extensionAuthor__",
    "background": {
        "scripts": [
            "common.js",
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "common.js",
                "content.js"
            ],
            "match_about_blank": true,
            "run_at": "document_start"
        }
    ],
    "default_locale": "en_US",
    "description": "__MSG_extensionDescription__",
    "homepage_url": "https://github.com/pea3nut/two-fingers-gestures/",
    "icons": {
        "256": "favicon@256.png"
    },
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "options_ui": {
        "page": "options.html",
        "browser_style" :true
    },
    "permissions": [
        "storage"
    ],
    "version": "0.1.1"
}
